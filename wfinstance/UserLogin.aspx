    <script>

        $(function () {
            var day = formatDateTime(new Date());
            $('#ArchivedOn').val(day);
            //var fileDate = getQueryString("CreatedOn");
            //$('#ArchivedOn').val(day); 

        });


        function formatDateTime(date) {

            var y = date.getFullYear();

            var m = date.getMonth() + 1;

            m = m < 10 ? ('0' + m) : m;

            var d = date.getDate();

            d = d < 10 ? ('0' + d) : d;

            var h = date.getHours();

            h = h < 10 ? ('0' + h) : h;

            var minute = date.getMinutes();

            minute = minute < 10 ? ('0' + minute) : minute;

            var second = date.getSeconds();

            second = second < 10 ? ('0' + second) : second;

            return y + '-' + m + '-' + d + ' ' + h + ':' + minute + ':' + second;

        }

        function closeDialog(refreshParent) {
            if (window.opener) {
                window.opener.closeDialog(refreshParent);
            } else {
                window.parent.closeDialog(refreshParent);
            }
        }

        function validate() {
            var username = $('#username').textbox('getValue'); 
            var pwd = $('#pwd').passwordbox('getValue'); 
            if (username == "") {
                $.messager.alert('提示', '请输入用户名!', 'error');
                return;
            }


            if (pwd == "") {
                $.messager.alert('提示', '请输入密码!', 'error');
                return;
            }

           
            var param = { username: username, pwd: pwd };
            var url = "oa.user.login.validation";

            ajaxMethodPOSTData(url, param, function (data) {
                if (data.status == 1) {
                    validateSuccess();
                }
                else {
                    $.messager.alert('提示', data.msg);
                }
            })

        }
        function validateSuccess()
        {
            //closeDialog(true);
            var id = document.getElementById("id").value;
            var t = document.getElementById("t").value;
            var url = "/apps/RecDetail.aspx?t=" + t + "&id=" + id;
            window.open(url);
            window.parent.closeDialog();
            window.close();

        }

        function myformatter(date) {
            var y = date.getFullYear();
            var m = date.getMonth() + 1;
            var d = date.getDate();
            return y + '-' + (m < 10 ? ('0' + m) : m) + '-' + (d < 10 ? ('0' + d) : d);
        }
        function myparser(s) {
            if (!s) return new Date();
            var ss = (s.split('-'));
            var y = parseInt(ss[0], 10);
            var m = parseInt(ss[1], 10);
            var d = parseInt(ss[2], 10);
            if (!isNaN(y) && !isNaN(m) && !isNaN(d)) {
                return new Date(y, m - 1, d);
            } else {
                return new Date();
            }
        }
    </script>
