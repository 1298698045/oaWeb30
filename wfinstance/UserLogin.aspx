             <tr>
                <td style="text-align:right">密码:</td>
                <td><input id="pwd"  class="easyui-passwordbox"   style="width:200px;" />
                </td>
            </tr>
        </table>
      
    </div>
           
        <div><p>
            <input type="button" id="btnSave"  name="btnSave" value="确认" onclick="validate()"  />
